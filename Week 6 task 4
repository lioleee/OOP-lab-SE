public class Maximum {


    public static void main(String[] args) {
      Maximum m=new Maximum();
      int a=12;
      int b=20;
      int c=13;
      if(a>b&& a>c){
                   System.out.println("A is biggest"+a);}
      
          else if(b>a &&b>c)
      {
                            System.out.println("B is Biggest"+b);
                  } 
          else
    System.out.println("C is biggest"+c);}
    
}
